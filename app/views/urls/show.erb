<div class="container">
  <div class="row well">
    <div class="col-md-12">
      <h1 id="header">Url Specific Data</h1>
      <h1 ><%= @source.identifier%>/<%= @path %></h1>
    </div>
  </div>
</div>

<div class="container">
  <div class="row well">
    <div class="col-md-12">
      <h1><%= @source.identifier%>/<%= @path %></h1>
    </div>
  </div>
</div>

<div class="container">
  <div class="row well">
    <div class="col-md-12">
      <h4>Longest Response Time: <%= @longest_response_time.to_i %></h4>
      <h4>Shortest Response Time: <%= @shortest_response_time.to_i %></h4>
      <h4>Average Response Time: <%= @average_response_time.to_i %></h4>
      <h4>HTTP Verbs:</h4>
      <% @requests.each do |request| %>
        <%= request %>
      </br>
        <% end %>
        <h4>Most Popular Referrers: </h4>
        <% @most_popular_referrers.each do |referrer| %>
          <%= referrer %>
          </br>
        <% end %>
        <h4>Most Popular Browsers: </h4>
        <% @most_popular_browsers.each do |browser| %>
          <%= browser %>
          </br>
        <% end %>

        <h4>Most Popular Operating Systems: </h4>
        <% @most_popular_os.each do |os| %>
          <%= os %>
          </br>
        <% end %>
    </div>
  </div>
</div>
